{
	"name": "tempo-app",
	"private": true,
	"type": "module",
	"imports": {
		"#*": "./src/*",
		"#wrangler.json": "./wrangler.jsonc"
	},
	"scripts": {
		"postinstall": "pnpm gen:types",
		"dev": "pnpm dev:moderato",
		"dev:devnet": "CLOUDFLARE_ENV='devnet' vite dev --port 3001",
		"dev:moderato": "CLOUDFLARE_ENV='moderato' vite dev --port 3001",
		"dev:presto": "CLOUDFLARE_ENV='presto' vite dev --port 3001",
		"build": "vite build",
		"check": "pnpm check:biome && pnpm check:types",
		"check:biome": "biome check --write --unsafe",
		"check:types": "tsgo --project tsconfig.json --noEmit",
		"predeploy": "pnpm build",
		"deploy": "wrangler deploy",
		"gen:types": "test -f .env || cp .env.example .env; CLOUDFLARE_ENV= wrangler types",
		"preview": "vite preview --mode='preview'"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.1.18",
		"@tanstack/react-query": "^5.90.16",
		"@tanstack/react-router": "^1.149.3",
		"@tanstack/react-router-ssr-query": "^1.149.3",
		"@tanstack/react-start": "^1.149.3",
		"@tanstack/router-plugin": "^1.149.3",
		"@wagmi/core": "^3.2.2",
		"animejs": "^4.2.2",
		"cva": "^1.0.0-beta.4",
		"ox": "^0.11.3",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.18",
		"uqr": "^0.1.2",
		"viem": "^2.44.2",
		"wagmi": "^3.3.2",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@cloudflare/vite-plugin": "^1.20.3",
		"@iconify/json": "^2.2.427",
		"@types/animejs": "^3.1.13",
		"@svgr/core": "^8.1.0",
		"@svgr/plugin-jsx": "^8.1.0",
		"@tanstack/devtools-vite": "^0.4.1",
		"@tanstack/react-devtools": "^0.9.2",
		"@tanstack/react-query-devtools": "^5.91.2",
		"@tanstack/react-router-devtools": "^1.149.3",
		"@types/node": "^25.0.8",
		"@types/react": "^19.2.8",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.2",
		"jsonc-parser": "^3.3.1",
		"tw-animate-css": "^1.4.0",
		"typed-query-selector": "^2.12.0",
		"unplugin-icons": "^22.5.0",
		"vite": "npm:rolldown-vite@latest",
		"wrangler": "^4.59.1"
	}
}
