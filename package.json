{
	"name": "tempo-app",
	"private": true,
	"type": "module",
	"packageManager": "pnpm@10.28.0",
	"imports": {
		"#*": "./src/*",
		"#wrangler.json": "./wrangler.json"
	},
	"scripts": {
		"postinstall": "pnpm gen:types",
		"dev": "pnpm dev:presto",
		"dev:devnet": "CLOUDFLARE=1 VITE_TEMPO_ENV=devnet waku dev --port 3001",
		"dev:moderato": "CLOUDFLARE=1 VITE_TEMPO_ENV=moderato waku dev --port 3001",
		"dev:presto": "CLOUDFLARE=1 VITE_TEMPO_ENV=presto waku dev --port 3001",
		"build": "CLOUDFLARE=1 waku build",
		"bundle:analyze": "ANALYZE=true pnpm build",
		"bundle:analyze:json": "ANALYZE_JSON=true pnpm build",
		"bundle:diff": "node --experimental-strip-types scripts/bundle-diff.ts",
		"bundle:save": "node --experimental-strip-types scripts/bundle-diff.ts --save",
		"check": "pnpm check:biome && pnpm check:types",
		"check:biome": "biome check --write --unsafe",
		"check:types": "tsgo --project tsconfig.json --noEmit",
		"predeploy": "pnpm build",
		"deploy": "wrangler deploy",
		"gen:types": "test -f .env || cp .env.example .env; CLOUDFLARE_ENV= wrangler types",
		"preview": "waku start --port 3001"
	},
	"dependencies": {
		"@emoji-mart/data": "^1.2.1",
		"@emoji-mart/react": "^1.1.1",
		"@tanstack/react-query": "^5.90.17",
		"@wagmi/core": "^3.2.2",
		"@walletconnect/ethereum-provider": "^2.23.3",
		"animejs": "^4.2.2",
		"i18next": "^25.2.1",
		"idxs": "^0.0.6",
		"jose": "^6.1.0",
		"ox": "^0.11.3",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"react-i18next": "^15.5.3",
		"uqr": "^0.1.2",
		"viem": "^2.44.4",
		"wagmi": "^3.3.2",
		"waku": "1.0.0-alpha.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@cloudflare/vite-plugin": "^1.21.0",
		"@iconify/json": "^2.2.428",
		"@svgr/core": "^8.1.0",
		"@svgr/plugin-jsx": "^8.1.0",
		"@tailwindcss/vite": "^4.1.18",
		"@tanstack/react-query-devtools": "^5.91.2",
		"@total-typescript/ts-reset": "^0.6.1",
		"@types/animejs": "^3.1.13",
		"@types/node": "^25.0.9",
		"@types/react": "^19.2.8",
		"@types/react-dom": "^19.2.3",
		"@typescript/native-preview": "^7.0.0-dev.20260115.1",
		"@vitejs/plugin-react": "^5.1.2",
		"rollup-plugin-visualizer": "^6.0.5",
		"sonda": "^0.10.1",
		"tailwindcss": "^4.1.18",
		"tw-animate-css": "^1.4.0",
		"typed-query-selector": "^2.12.0",
		"unplugin-icons": "^23.0.1",
		"vite": "npm:rolldown-vite@latest",
		"wrangler": "^4.59.2",
		"zod": "^4.3.5"
	},
	"pnpm": {
		"peerDependencyRules": {
			"allowedVersions": {
				"react": "^19.0.0",
				"react-dom": "^19.0.0"
			}
		}
	}
}
