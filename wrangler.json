{
	"$schema": "https://esm.sh/wrangler/config-schema.json",
	"name": "app-presto",
	"compatibility_date": "2025-12-17",
	"compatibility_flags": ["nodejs_compat"],
	"main": "./src/index.server.ts",
	"keep_vars": true,
	"workers_dev": true,
	"preview_urls": true,
	"placement": { "mode": "smart" },
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "app-db-devnet",
			"database_id": "acb1c13a-5666-4974-954b-ad308d3ba1b8"
		}
	],
	"vars": {
		"VITE_TEMPO_ENV": "presto"
	},
	"env": {
		"devnet": {
			"name": "app-devnet",
			"main": "./src/index.server.ts",
			"routes": [
				{
					"custom_domain": true,
					"zone_name": "tempo.xyz",
					"pattern": "app.devnet.tempo.xyz"
				}
			],
			"d1_databases": [
				{
					"binding": "DB",
					"database_name": "app-db",
					"database_id": "placeholder-update-after-create"
				}
			],
			"vars": {
				"VITE_TEMPO_ENV": "devnet"
			},
			"workers_dev": true,
			"observability": {
				"enabled": true,
				"logs": {
					"persist": true,
					"enabled": true,
					"head_sampling_rate": 1,
					"invocation_logs": true
				}
			}
		},
		"moderato": {
			"name": "app-moderato",
			"main": "./src/index.server.ts",
			"routes": [
				{
					"custom_domain": true,
					"zone_name": "tempo.xyz",
					"pattern": "app.moderato.tempo.xyz"
				}
			],
			"d1_databases": [
				{
					"binding": "DB",
					"database_name": "app-db",
					"database_id": "placeholder-update-after-create"
				}
			],
			"vars": {
				"VITE_TEMPO_ENV": "moderato"
			},
			"workers_dev": true,
			"observability": {
				"enabled": true,
				"logs": {
					"persist": true,
					"enabled": true,
					"head_sampling_rate": 1,
					"invocation_logs": true
				}
			}
		},
		"presto": {
			"name": "app-presto",
			"main": "./src/index.server.ts",
			"routes": [
				{
					"custom_domain": true,
					"zone_name": "tempo.xyz",
					"pattern": "app.tempo.xyz"
				},
				{
					"custom_domain": true,
					"zone_name": "tempo.xyz",
					"pattern": "app.presto.tempo.xyz"
				},
				{
					"custom_domain": true,
					"zone_name": "tempo.xyz",
					"pattern": "app.mainnet.tempo.xyz"
				}
			],
			"d1_databases": [
				{
					"binding": "DB",
					"database_name": "app-db",
					"database_id": "placeholder-update-after-create"
				}
			],
			"vars": {
				"VITE_TEMPO_ENV": "presto"
			},
			"workers_dev": true,
			"observability": {
				"enabled": true,
				"logs": {
					"persist": true,
					"enabled": true,
					"head_sampling_rate": 1,
					"invocation_logs": true
				}
			}
		}
	},
	"observability": {
		"enabled": true,
		"logs": {
			"enabled": true,
			"head_sampling_rate": 1,
			"invocation_logs": true,
			"persist": true
		}
	}
}
