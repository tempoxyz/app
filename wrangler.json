{
	"$schema": "https://esm.sh/wrangler/config-schema.json",
	"name": "app-presto",
	"compatibility_date": "2025-12-17",
	"compatibility_flags": ["nodejs_compat"],
	"main": "dist/server/serve-cloudflare.js",
	"assets": { "directory": "dist/public" },
	"keep_vars": true,
	"workers_dev": true,
	"preview_urls": true,
	"placement": { "mode": "smart" },
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "app-db-presto",
			"database_id": "1c1dbae2-1cce-49ea-991d-9a502b753d3e"
		}
	],
	"vars": {
		"VITE_TEMPO_ENV": "presto",
		"PRESTO_RPC_AUTH": "",
		"HTTP_AUTH": ""
	},
	"env": {
		"devnet": {
			"name": "app-devnet",
			"keep_vars": true,
			"main": "dist/server/serve-cloudflare.js",
	"assets": { "directory": "dist/public" },
			"routes": [
				{
					"custom_domain": true,
					"zone_name": "tempo.xyz",
					"pattern": "app.devnet.tempo.xyz"
				}
			],
			"d1_databases": [
				{
					"binding": "DB",
					"database_name": "app-db",
					"database_id": "placeholder-update-after-create"
				}
			],
			"vars": {
				"VITE_TEMPO_ENV": "devnet"
			},
			"workers_dev": true,
			"observability": {
				"enabled": true,
				"logs": {
					"persist": true,
					"enabled": true,
					"head_sampling_rate": 1,
					"invocation_logs": true
				}
			}
		},
		"moderato": {
			"name": "app-moderato",
			"keep_vars": true,
			"main": "dist/server/serve-cloudflare.js",
	"assets": { "directory": "dist/public" },
			"routes": [
				{
					"custom_domain": true,
					"zone_name": "tempo.xyz",
					"pattern": "app.moderato.tempo.xyz"
				}
			],
			"d1_databases": [
				{
					"binding": "DB",
					"database_name": "app-db-moderato",
					"database_id": "ffba4a97-9f84-45bb-b814-3dc5cb6e378f"
				}
			],
			"vars": {
				"VITE_TEMPO_ENV": "moderato"
			},
			"workers_dev": true,
			"observability": {
				"enabled": true,
				"logs": {
					"persist": true,
					"enabled": true,
					"head_sampling_rate": 1,
					"invocation_logs": true
				}
			}
		},
		"presto": {
			"name": "app-presto",
			"keep_vars": true,
			"main": "dist/server/serve-cloudflare.js",
	"assets": { "directory": "dist/public" },
			"routes": [
				{
					"custom_domain": true,
					"zone_name": "tempo.xyz",
					"pattern": "app.tempo.xyz"
				},
				{
					"custom_domain": true,
					"zone_name": "tempo.xyz",
					"pattern": "app.presto.tempo.xyz"
				},
				{
					"custom_domain": true,
					"zone_name": "tempo.xyz",
					"pattern": "app.mainnet.tempo.xyz"
				}
			],
			"d1_databases": [
				{
					"binding": "DB",
					"database_name": "app-db-presto",
					"database_id": "1c1dbae2-1cce-49ea-991d-9a502b753d3e"
				}
			],
			"vars": {
				"VITE_TEMPO_ENV": "presto"
			},
			"workers_dev": true,
			"observability": {
				"enabled": true,
				"logs": {
					"persist": true,
					"enabled": true,
					"head_sampling_rate": 1,
					"invocation_logs": true
				}
			}
		}
	},
	"observability": {
		"enabled": true,
		"logs": {
			"enabled": true,
			"head_sampling_rate": 1,
			"invocation_logs": true,
			"persist": true
		}
	}
}
